<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFrameworks>net472;netcoreapp3.1</TargetFrameworks>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\check-32.png" />
    <None Remove="Resources\checked-checkbox-32.png" />
    <None Remove="Resources\delete-24-red.png" />
    <None Remove="Resources\delete-24-redxx.png" />
    <None Remove="Resources\edit-32.png" />
    <None Remove="Resources\edit-8-32.png" />
    <None Remove="Resources\trash-32.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\check-32.png" />
    <Resource Include="Resources\checked-checkbox-32.png" />
    <Resource Include="Resources\delete-24-red.png" />
    <Resource Include="Resources\delete-24-redxx.png" />
    <Resource Include="Resources\edit-32.png" />
    <Resource Include="Resources\edit-8-32.png" />
    <Resource Include="Resources\trash-32.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>